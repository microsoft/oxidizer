fn test_method(&self) -> Result<(), OhnoErrorType> {
    (|| {
        let state = self.get_state()?;
        state.validate()?;
        Ok(())
    })()
        .map_err(|mut e| {
            let msg = "error in function test_method";
            ohno::Enrichable::add_enrichment(
                &mut e,
                ohno::EnrichmentEntry::new(msg, file!(), line!()),
            );
            e
        })
}
